<STYLE TYPE="text/css">
.ymaps_balloon{
	color: #000000;
	width: 275px;
	padding-bottom:15px;
	font-size: 8pt;
	font-family: Tahoma;
	line-height:14px;
}
</STYLE>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<script src="http://api-maps.yandex.ru/2.0/?coordorder=longlat&load=package.full&lang=ru-RU" type="text/javascript"></script>
<script type="text/javascript" src="http://api.korzhevdp.com/jscript/map_styles2.js"></script>
<script type="text/javascript">
	ymaps.ready(display_locations);
	function display_locations(){
		map = new ymaps.Map("YMapsID", {
			center: [40.51643361,64.53838365],
			zoom: 17,
			type: 'yandex#satellite',
			behaviors: ["scrollZoom", "drag", "dblClickZoom"]
		});
		map.controls.add('mapTools').add('zoomControl');
		for (a in userstyles){
			ymaps.option.presetStorage.add(a, userstyles[a]);
		}
		ms = new ymaps.GeoObjectArray();
		var genericBalloon = ymaps.templateLayoutFactory.createClass(
			'<div class="ymaps_balloon">' +
			'<b>Название:</b> $[properties.name|без имени]<br><b>Адрес:</b> $[properties.address|нет]<br>' +
			'<b>Описание:</b> $[properties.description|без описания]' +
			'</div></div>'
		);
		ymaps.layout.storage.add('generic#balloonLayout', genericBalloon);
		ms.options.set({
			balloonContentBodyLayout: 'generic#balloonLayout',
			balloonMaxWidth: 300// Максимальная ширина балуна в пикселах
		});
		object = new ymaps.Polyline(new ymaps.geometry.LineString.fromEncodedCoordinates('EPNqAkK22ANiDgAAXgsAAGENAAD2DQAAI97__9UdAADf8P__PA0AAH32__8yCQAAJP___8AAAAAK_P__KAIAAAn____-AAAAwP___-cAAACzBQAAYQIAAAn___-7AAAAKP7__24AAABI_v__qgAAANfy__-7BgAA--3__2sJAABK2___QhIAAA=='), {address: 'Нет адреса', description: 'Нет адреса', name: 'Название', hasHint: 1, hintContent: 'Название Нет адреса', link: '' }, ymaps.option.presetStorage.get('metro#red'));
ms.add(object);
object = new ymaps.Polyline(new ymaps.geometry.LineString.fromEncodedCoordinates('iaFqAiGj2AMt7P__GQQAANzu__8iBAAABuD__8oKAACe4f__rAoAAEj-___JAAAAwfn__2cEAACq____GgEAAP_-___MAwAAav___wMEAADrBQAAgAoAALwHAAAcEAAAcggAAJgQAAAoEwAAkiIAAA=='), {address: 'Нет адреса', description: 'Нет адреса', name: 'Название', hasHint: 1, hintContent: 'Название Нет адреса', link: '' }, ymaps.option.presetStorage.get('metro#red'));
ms.add(object);		map.geoObjects.add(ms);
	}
</script>
<div id="YMapsID" style="width:100%;height:98%;border:1px solid #FFFFFF;margin:0px;"></div>
