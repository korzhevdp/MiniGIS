ms.add(object);
object = new ymaps.Polygon(new ymaps.geometry.Polygon.fromEncodedCoordinates("nm9qAmmj2AOXDwAAMP3__9oCAACNBQAAr_T__5YBAADg-P__rfv__w=="), {address: '', description: 'Ившина, Оперная дамба, товарищества по Ваге и Двине', name: 'Пристани', hasHint: 1, hintContent: 'Пристани Ившина, Оперная дамба, товарищества по Ваге и Двине', link: '' }, ymaps.option.presetStorage.get('twirl#redDotIcon'));
ms.add(object);
